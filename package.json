    "@angular/core": "^2.4.8",
    "@angular/forms": "https://registry.npmjs.org/@angular/forms/-/forms-2.4.8.tgz",
    "@angular/http": "2.2.1",
    "@angular/platform-browser": "2.2.1",
    "@angular/platform-browser-dynamic": "2.2.1",
    "@angular/router": "3.2.1",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-alpha.20",
    "@ngrx/core": "^1.2.0",
    "@ngrx/store": "^2.2.1",
    "core-js": "^2.4.1",
