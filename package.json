{
  "name": "daln-web-cli",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "globals": "npm cache clean && npm i -g angular-cli@1.0.0-beta.21 aws-sdk",
    "start": "ng serve",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "buildDev": "ng build",
    "buildProd": "ng build -prod",
    "s3Synch": "npm run buildDev && aws s3 sync ./dist s3://daln/dev-app",
    "s3Prod": "npm run buildProd && aws s3 sync ./dist s3://daln/prod"
  },
  "private": true,
  "dependencies": {
    "@angular/cli": "^1.0.0-beta.32",
